mixin about-us-card(data={ href:"#", img: { src:"", alt:"" }}, index=0)
	// About us card
	a.about-us.about-us__hover-effect.wow.fadeInUp(href=data.href, data-wow-duration=`1s`, data-wow-delay=`${(index + 1) * 0.2}s`)
		.about-us__image
			img(src=data.img.src, alt=data.img.alt)
		if(data.title)
			h5.about-us__title(data-swiper-parallax="-300")=data.title
	// About us card End/.
