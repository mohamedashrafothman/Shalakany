mixin pagination(data={})
	// Pagination
	nav.pagination.pagination--brown.wow.fadeInUp(data-wow-duration="1s", data-wow-delay="0s")
		ul.pagination__list
			li.pagination__list-item.pagination__counter
				span.pagination__number 1
				span.pagination__number 14
			li.pagination__list-item.pagination__prev
				a(href="#")
			li.pagination__list-item.pagination__next
				a(href="#")
	// Pagination End/.
